@model IEnumerable<foro_C.Models.Rol>

@{
    ViewData["Title"] = "roles";
}

<h1>Roles existentes</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
           
        </tr>
}
    </tbody>
</table>
